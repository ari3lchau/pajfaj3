<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %>
  

  <!-- Main Content -->
  <h1 class="text-center" id="suv_1">Survey 1 Result</h1><br>
  <main class="container-survey">
    
    <div>
      
        <div>
          <br>
            
            
            <% if(typeof displayName == 'string' && displayName) { %>
                
            <% } %><br>
            
            <br>
            <table class="table table-bordered table-striped" id="table_1">
                <!-- Header Row-->
                <tr>
                    <% if(typeof displayName == 'string' && displayName) { %>
                        <th class="text-center">Full Name</th>
                        <th class="text-center">Email</th>                        
                        <th class="text-center">Address</th>
                        <th class="text-center">Phone Number</th>
                        <th class="text-center">Age</th>
                    <% } else { %>
                        <th class="text-center">Full Name</th>
                        <th class="text-center">Email</th>                        
                        <th class="text-center">Address</th>
                        <th class="text-center">Phone Number</th>
                        <th class="text-center">Age</th>
                        <th></th>
                        <th></th>
                    <% } %>
                </tr>

                <!-- Repeatable Template Row -->

                <% for(let count = 0; count < SurveyList1.length; count++) { %>
                    <tr>
                        <% if(typeof displayName == "string" && displayName) { %>
                           <td class="text-center"><%= SurveyList1[count].fullname %></td>
                           <td class="text-center"><%= SurveyList1[count].email %></td>
                           <td class="text-center"><%= SurveyList1[count].address %></td>
                           <td class="text-center"><%= SurveyList1[count].phoneNumber %></td>
                           <td class="text-center"><%= SurveyList1[count].age %></td>
                
                           <td class="text-center">
                            <a href="/surveylist1/edit1/<%= SurveyList1[count]._id %>" class="btn btn-primary"> <i class="fa"></i>Edit</a>
                          </td>
                          <td class="text-center">
                            <a href="/surveylist1/delete/<%= SurveyList1[count]._id %>" class="btn btn-warning btn-sm"> <i class="fa"></i>Delete</a>
                          </td>
                         <% } else { %>
                            <td class="text-center"><%= SurveyList1[count].fullname %></td>
                            <td class="text-center"><%= SurveyList1[count].email %></td>
                            <td class="text-center"><%= SurveyList1[count].address %></td>
                            <td class="text-center"><%= SurveyList1[count].phoneNumber %></td>
                            <td class="text-center"><%= SurveyList1[count].age %></td>
                         <% } %>
                       </tr>
                <% } %>
            </table>

          
        </div>

        
    </div>
  </main>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      // Load google charts
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);
      
      // Draw the chart and set the chart values
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
        ['Task', 'Hours per Day'],
        ['17-20', 8],
        ['21-29', 2],
        ['30+', 2],
      ]);
      
        // Optional; add a title and set the width and height of the chart
        var options = {'title':'Age', 'width':550, 'height':400};
      
        // Display the chart inside the <div> element with id="piechart"
        var chart = new google.visualization.PieChart(document.getElementById('piechart'));
        chart.draw(data, options);
      }
      </script>
  <h1 class="text-center" id="suv_1_b">Pie Chart For Multiple Choices</h1>
  <div id="piechart"></div>
 
