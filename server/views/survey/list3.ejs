<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %>
  

<!-- Main Content -->
<h1 class="text-center" id="suv_3">Survey 3 Result</h1>
<main class="container-survey">
  <div>
      <div><br>
          
          
          <% if(typeof displayName == 'string' && displayName) { %>
              
          <% } %><br>
          
          <br>
          <table class="table table-bordered table-striped" id="table_3">
              <!-- Header Row-->
              <tr>
                  <% if(typeof displayName == 'string' && displayName) { %>
                      <th class="text-center">Field</th>
                      <th class="text-center">Major</th>                        
                      <th class="text-center">Institution</th>
                      <th class="text-center">Years</th>
                      
                  <% } else { %>
                    <th class="text-center">Field</th>
                    <th class="text-center">Major</th>                        
                    <th class="text-center">Institution</th>
                    <th class="text-center">Years</th>
                      <th></th>
                      <th></th>
                  <% } %>
              </tr>

              <!-- Repeatable Template Row -->

              <% for(let count = 0; count < SurveyList3.length; count++) { %>
                  <tr>
                      <% if(typeof displayName == "string" && displayName) { %>
                         <td class="text-center"><%= SurveyList3[count].field %></td>
                         <td class="text-center"><%= SurveyList3[count].major %></td>
                         <td class="text-center"><%= SurveyList3[count].institution %></td>
                         <td class="text-center"><%= SurveyList3[count].years %></td>
                         
              
                         <td class="text-center">
                          <a href="/surveylist3/edit3/<%= SurveyList3[count]._id %>" class="btn btn-primary"> <i class="fa"></i>Edit</a>
                        </td>
                        <td class="text-center">
                          <a href="/surveylist3/delete/<%= SurveyList3[count]._id %>" class="btn btn-warning btn-sm"> <i class="fa"></i>Delete</a>
                        </td>
                       <% } else { %>
                          <td class="text-center"><%= SurveyList3[count].field %></td>
                          <td class="text-center"><%= SurveyList3[count].major %></td>
                          <td class="text-center"><%= SurveyList3[count].institution %></td>
                          <td class="text-center"><%= SurveyList3[count].years %></td>
                       <% } %>
                     </tr>
              <% } %>
          </table>
      </div>
  </div>
</main>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      // Load google charts
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);
      
      // Draw the chart and set the chart values
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
        ['Task', 'Hours per Day'],
        ['Science', 8],
        ['Commerce', 2],
        ['Arts', 2],
      ]);
      
        // Optional; add a title and set the width and height of the chart
        var options = {'title':'Field', 'width':550, 'height':400};
      
        // Display the chart inside the <div> element with id="piechart"
        var chart = new google.visualization.PieChart(document.getElementById('piechart'));
        chart.draw(data, options);
      }
      </script>
  <h1 class="text-center" id="suv_1_b">Pie Chart For Multiple Choices</h1>
  <div id="piechart"></div>

